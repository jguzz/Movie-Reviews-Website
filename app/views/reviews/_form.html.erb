<%= form_for @review do |f| %>
<%= f.label :rating %>
<%= f.number_field :rating %>
<br>
<%= f.label :description %>
<%= f.text_area :description %>
<br>
<%= f.label :likes %>
<%= f.number_field :likes %>
<br>
<%= f.label :movie_id %>
<%= f.collection_select :movie_id, Movie.all, :id, :title %>
<br>
<%= f.label :user_id %>
<%= f.collection_select :user_id, User.all , :id, :name %>
<br>
<%= f.submit %>
<% end %>